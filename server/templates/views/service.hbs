<div class="container">
	<h1>Services</h1>
	<div class="row">
		<div class="col-md-12 col-lg-12">
			<div class="row">
				{{#each services}}
				<div class="col-md-4 col-lg-4">
					<h2><a href="serviceUrl slug">{{{name}}}</a></h2>
					{{#if image}}
						<img src="{{{cloudinaryUrl image}}}" class="thumbnail">
					{{/if}}
				</div>
				{{/each}}
			</div>
		</div>
	</div>
</div>
